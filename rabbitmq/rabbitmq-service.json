{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "rabbitmq",
    "labels": {
      "name": "rabbitmq"
    }
  },
  "spec": {
    "ports": [

      {
        "name": "rabbitmq-main",
        "port": 5672,
        "targetPort": 5672
      },
    	{
        "name": "mqtt",
        "port": 1883,
        "targetPort": 1883
      },
    	{
        "name": "mgmt",
        "port": 15672,
        "targetPort": 15672
      }
    ],
    "selector": {
      "name": "rabbitmq-controller"
    }
  }
}
